{
  "name": "@zeitgeistpm/utility",
  "version": "2.0.19",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "yarn compile",
    "compile": "rimraf ./dist && tsup",
    "generate-exports": "node ./scripts/generate_exports.mjs",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "@polkadot/api": "*",
    "@polkadot/types": "*",
    "@polkadot/util": "*"
  },
  "dependencies": {
    "bignumber.js": "^9.1.0",
    "lodash.omit": "^4.5.0",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@types/lodash.omit": "^4.5.7"
  }
}
