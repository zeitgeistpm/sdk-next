{
  "name": "@zeitgeistpm/augment-api",
  "version": "0.8.0",
  "description": "Generated types for the Zeitgeist network.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn generate:defs && yarn generate:meta && rimraf ./dist && tsc",
    "generate:defs": "ts-node --skip-project ../../node_modules/.bin/polkadot-types-from-defs --package @zeitgeistpm/augment-api --input ./src/interfaces --endpoint ./src/metadata/latest.json",
    "generate:meta": "ts-node --skip-project ../../node_modules/.bin/polkadot-types-from-chain --package @zeitgeistpm/augment-api --endpoint ./src/metadata/latest.json --output ./src/interfaces  --strict",
    "fetch:metadata": "node ./scripts/fetchMetadata.js -o './src/metadata/latest.json' -e 'wss://bsr.zeitgeist.pm'"
  },
  "author": "Zeitgeist PM <contact@zeitgeist.pm>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@polkadot/typegen": "9.2.2"
  },
  "dependencies": {
    "@polkadot/types": "9.2.2",
    "@zeitgeistpm/type-defs": "^0.8.0",
    "axios": "^0.21.1"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./interfaces/predictionMarkets": {
      "default": "./dist/interfaces/predictionMarkets/index.js"
    },
    "./interfaces/predictionMarkets.d.ts": {
      "default": "./dist/interfaces/predictionMarkets/index.d.ts"
    }
  },
  "gitHead": "f65183993419e36276711f4634c57536ffad93a1"
}
